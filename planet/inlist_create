! inlist_create

&star_job


      show_log_description_at_start = .false.

      save_model_when_terminate = .true.
      save_model_filename = "planet_create.mod"
      
      create_initial_model = .true.
      radius_in_cm_for_create_initial_model = 13982200000.0d0  ! 13982200000.d0 ! in cm
      mass_in_gm_for_create_initial_model = 1.8389d30 ! in grams
      initial_model_relax_num_steps = 0
      history_columns_file = 'history_columns.list'

	
      set_initial_age = .true.
      initial_age = 0.d0 ! in years

      eos_file_prefix = 'mesa'
      kappa_file_prefix = 'gs98'
      kappa_lowT_prefix = 'lowT_Freedman11'

      change_lnPgas_flag = .true.
      new_lnPgas_flag = .true.
      
      !pgstar_flag = .true.

/ !end of star_job

&controls


	initial_Z = 0.02d0
